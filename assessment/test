add2(X-Y,R,Z) :- expand(X,0,A),minus(Y,0,B),expand(R,0,C),add2(A,B,D),add2(D,C,Z).
add2(X,Y-R,Z) :- expand(X,0,A),expand(Y,0,B),minus(R,0,C),add2(A,B,D),add2(D,C,Z).
